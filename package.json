{
  "name": "vui-icons",
  "version": "2.0.0",
  "description": "Web components, mixins and image assets for consuming icons",
  "private": true,
  "scripts": {
    "prebuild": "rimraf *.css",
    "build": "npm run build:scss && npm run build:css-scss",
    "build:scss": "node ./cli/create-scss-cli -o icons.scss images/**/*.*",
    "build:css-scss": "node ./cli/create-css-scss-cli -o icons.css.scss images/**/*.*",
    "build:iconsets": "node ./cli/create-iconset-cli images/tier1 images/tier2 images/tier3 images/html-editor",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-icons.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-icons/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-icons",
  "devDependencies": {
    "chalk": "^1.1.1",
    "fs": "0.0.2",
    "imagemin": "^4.0.0",
    "images-to-variables": "^0.3.0",
    "q": "^1.4.1",
    "rimraf": "^2.5.2",
    "through2": "^2.0.1",
    "vinyl-fs": "^2.3.1",
    "xml2js": "^0.4.16",
    "yargs": "^4.1.0"
  }
}
