{
  "name": "vui-icons",
  "version": "0.4.1",
  "description": "Mixins, CSS and image assets for consuming icons",
  "scripts": {
    "clean": "rimraf *.css && rimraf *.less",
    "prebuild": "npm run clean",
    "build": "npm run build:less:css && npm run build:sass:css",
    "build:less": "node ./cli/create-less-cli -o icons.less images/**/*.png",
    "build:less:vui": "node ./cli/create-css-less-cli -o icons.css.less images/**/*.png",
    "prebuild:less:css": "npm run build:less && npm run build:less:vui",
    "build:less:css": "lessc icons.css.less > icons-less.css",
    "postbuild:less:css": "csslint icons-less.css",
    "build:sass": "node ./cli/create-scss-cli -o icons.scss images/**/*.png",
    "build:sass:vui": "node ./cli/create-css-scss-cli -o icons.css.scss images/**/*.png",
    "prebuild:sass:css": "npm run build:sass && npm run build:sass:vui",
    "build:sass:css": "node-sass --output-style expanded icons.css.scss icons.css",
    "postbuild:sass:css": "csslint icons.css",
    "pretest": "npm run build",
    "test": "npm run diff:assert && karma start karma.conf.js",
    "prediff:assert": "npm run diff:create",
    "diff:assert": "assertnodiff --file1 ./test/diff/less-scss-diff --file2 ./test/diff/less-scss-diff-new",
    "diff:create": "createpatch --file1 icons-less.css --file2 icons.css -o ./test/diff/less-scss-diff-new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-icons.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-icons/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-icons",
  "devDependencies": {
    "chalk": "^0.5.1",
    "csslint": "^0.10.0",
    "diff-cli": "git://github.com/Brightspace/diff-cli.git#v0.0.1",
    "fs": "0.0.2",
    "jasmine-dom-matchers": "^0.1.0",
    "imagemin": "^3.1.0",
    "images-to-variables": "^0.2.2",
    "karma": "^0.12.32",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-cli": "0.0.4",
    "less": "^2.5.1",
    "node-sass": "^3.1.1",
    "q": "^1.0.1",
    "rimraf": "^2.3.4",
    "through2": "^0.6.2",
    "vinyl-fs": "^0.3.7",
    "yargs": "^3.10.0"
  }
}
